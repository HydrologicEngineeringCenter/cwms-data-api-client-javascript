{
  "name": "cwms_js",
  "version": "0.1.0",
  "description": "Javascript Library for the CWMS REST API for Data Retrieval. This library is generated from the CWMS Swagger definition using OpenAPI Generator. The library is intended to be used in a browser environment.",
  "license": "MIT License",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "clean": "npx rimraf dist src",
    "dist": "babel src -d dist",
    "prepare": "npm run openapi && npm run dist",
    "test": "mocha --require @babel/register --recursive",
    "openapi": "npm run clean && openapi-generator-cli generate -g javascript -o . -i ../cwms-swagger.json --additional-properties=projectName=CWMSJS,npmRepository=CWMSJS,npmName=cwms_js",
    "validate": "openapi-generator-cli validate -i ../cwms-swagger.json",
    "generate": "npm run openapi",
    "bundle": "browserify -t [ babelify --presets [@babel/preset-env] ] src/index.js -o src/bundle.js"
    
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "superagent": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^8.0.1",
    "rimraf": "^5.0.1",
    "sinon": "^7.2.0"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE",
    "docs"
  ]
}
